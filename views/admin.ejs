<!doctype html>
<html>
<head><title>Admin</title></head>
<body>
  <h2>Admin Panel</h2>
  <p><a href="/dashboard">Back</a></p>
  <h3>Users</h3>
  <ul>
    <% users.forEach(u => { %>
      <li><%= u.email %> - admin: <%= u.isAdmin %> - <a href="/admin/view_creds/<%= u._id %>">view site creds</a></li>
    <% }) %>
  </ul>

  <h3>Recent Jobs</h3>
  <ul>
    <% jobs.forEach(j => { %>
      <li>Job <a href="/job/<%= j._id %>"><%= j._id %></a> - user <%= j.userId %> - <%= j.status %></li>
    <% }) %>
  </ul>
</body>
</html>
